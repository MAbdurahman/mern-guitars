    "express-mongo-sanitize": "^2.0.2",
    "express-validator": "^6.10.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.14",
    "mongoose": "^5.12.1",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
